d</Form.Label> */}\r\n        <p><Form.Control type=\"text\" placeholder=\"enter uid\" name=\"uid\" onChange={doUpdate} value={medObj.address} style={{width:\"20%\",margin:\"1rem auto\",display:\"inline-block\"}}/>\r\n        <Button type=\"button\" onClick={doFetch} style={{textAlign:\"center\"}} style={btn}>fetch</Button></p>\r\n        </Form.Group>\r\n        </Form>\r\n        {loading ?(\r\n        <Container>\r\n        <Row>\r\n         <Table style={tablee}>\r\n              <thead>             \r\n                  <tr>\r\n                  <th>Uid</th>\r\n                    <th>medicine name</th>\r\n                    <th>company</th>\r\n                    <th>exp date</th>\r\n                    <th>quantity</th>\r\n                    <th>units</th>\r\n                    <th>city</th>\r\n                    <th>picname</th>\r\n                    <th>delete</th>\r\n                  </tr>                  \r\n                </thead>\r\n                {\r\n                  jsonAry.map((obj) => {\r\n                   return (\r\n                   <tr style={row}>\r\n                    <td>{obj.uid}</td>\r\n                    <td>{obj.medname}</td>\r\n                    <td>{obj.company}</td>\r\n                    <td> {obj.expdate}</td>\r\n                    <td> {obj.quantity}</td>\r\n                    <td> {obj.units}</td>\r\n                    <td> {obj.city}</td>\r\n                    <td> {obj.mpic}</td>\r\n                     <td><Button style={btn} onClick={()=>doDelete(obj.uid,obj.medname)}>Delist</Button></td>\r\n                    </tr>\r\n              );\r\n            })}\r\n          </Table>\r\n        </Row>\r\n      </Container>)\r\n      :\r\n      (\r\n        <center>\r\n         <Loader/>\r\n        </center>\r\n      )\r\n        }\r\n        </>\r\n    )\r\n}\r\nexport default FetchMed;","D:\\Avishka\\housemed-website\\reactproject\\src\\Components\\SearchMed.jsx",["141","142","143"],"import React, { useState,use